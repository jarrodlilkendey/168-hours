{
    "private": true,
    "scripts": {
        "dev": "next dev",
        "lint": "next lint",
        "build": "next build",
        "build:test": "npm run prisma:seed && env-cmd -f .env.test.local next build",
        "start": "next start",
        "prisma:init": "npx prisma migrate dev --name init --schema prisma/schema.prisma",
        "prisma:migrate:deploy": "npx prisma migrate deploy",
        "prisma:migrate:dev": "npx prisma migrate dev",
        "prisma:seed": "env-cmd -f .env.test.local npx prisma db seed",
        "start:test": "env-cmd -f .env.test.local next start",
        "test:api": "jest --ci --runInBand -c ./__tests__/api/jest.config.js",
        "test:db:migrate": "npx prisma migrate reset --force --skip-generate --skip-seed",
        "test:ui": "jest --maxWorkers=2 --ci -c ./__tests__/ui/jest.config.js",
        "test:e2e:headless": "cypress:headless",
        "cypress:build": "npm run build:test && npm run cypress:start",
        "cypress:open": "env-cmd -f .env.test.local cypress open",
        "cypress:start": "npx browserslist@latest --update-db && start-server-and-test 'npm run start:test' 3000 'npm run cypress:open'",
        "cypress:run": "npm run build:test && start-server-and-test 'npm run start:test' 3000 'env-cmd -f .env.test.local cypress run'",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.4.2",
        "@prisma/client": "^5.14.0",
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-label": "^2.0.2",
        "@radix-ui/react-popover": "^1.0.7",
        "@radix-ui/react-slot": "^1.0.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "cypress": "^9.7.0",
        "date-fns": "^3.6.0",
        "env-cmd": "^10.1.0",
        "fast-json-patch": "^3.1.1",
        "jsonwebtoken": "^9.0.2",
        "lucide-react": "^0.379.0",
        "next": "latest",
        "next-auth": "^4.24.7",
        "next-connect": "^0.13.0",
        "next-themes": "^0.3.0",
        "pbkdf2": "^3.1.2",
        "react": "^18.3.1",
        "react-day-picker": "^8.10.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.51.5",
        "recharts": "^2.12.7",
        "server-only": "^0.0.1",
        "swr": "^2.2.5",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@testing-library/cypress": "^8.0.7",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/react": "14.3.1",
        "@testing-library/user-event": "^14.5.2",
        "@types/jest": "29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^20.12.12",
        "@types/pbkdf2": "^3.1.2",
        "@types/react": "18.3.3",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-next": "^14.2.3",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-testing-library": "^6.2.2",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "msw": "^1.3.3",
        "next-test-api-route-handler": "^3.1.10",
        "postcss": "^8.4.38",
        "prisma": "^5.14.0",
        "start-server-and-test": "^2.0.3",
        "tailwindcss": "^3.4.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "prisma": {
        "seed": "ts-node --project ./node.tsconfig.json ./prisma/seed.ts",
        "schema": "./prisma/schema.prisma"
    }
}
