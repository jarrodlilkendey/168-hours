// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Mobster {
  id    Int     @id @default(autoincrement())
  name  String
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  address String?
  phone String?
  token String?
  salt String
  hash String
  iterations Int
  keylen Int
  digest String
}